{
  "kind": "collectionType",
  "collectionName": "questions",
  "info": {
    "singularName": "question",
    "pluralName": "questions",
    "displayName": "Question",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "part": {
      "default": 0,
      "type": "integer",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "fill",
        "multipleChoice",
        "mix"
      ],
      "required": true
    },
    "typeSingleQuestion": {
      "type": "enumeration",
      "enum": [
        "fill",
        "multipleChoice"
      ]
    },
    "audio": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "audios"
      ]
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "subAudio": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "audios"
      ]
    },
    "question": {
      "type": "text",
      "required": true
    },
    "translate": {
      "type": "text",
      "required": true
    },
    "solve": {
      "type": "text",
      "required": true
    },
    "active": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "partitionCode": {
      "type": "string",
      "required": true
    },
    "children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::child-question.child-question",
      "mappedBy": "parentId"
    },
    "answers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::answer.answer",
      "mappedBy": "questionId"
    },
    "pointLadder": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::point-ladder-exam.point-ladder-exam",
      "mappedBy": "questionId"
    }
  }
}
